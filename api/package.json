{
  "name": "tournament-tracker-api",
  "version": "2.0.0",
  "description": "Tournament Tracker backend",
  "main": "./src/server.js",
  "scripts": {
    "start": "export NODE_ENV=production; node ./src/server.js",
    "dev": "export NODE_ENV=development; node --watch ./src/server.js",
    "sync": "node -e 'require(\"./src/services/init.services.js\")(true)'",
    "devdb": "cockroach start-single-node --insecure --store=path=\"$PWD/cockroach-data\" --listen-addr=localhost:26257 --http-addr=localhost:9095",
    "test": "jest",
    "inspect": "node --inspect ./src/server.js",
    "initdb": "cockroach sql --host=localhost:26257 --user=root --database=defaultdb --insecure -e \"CREATE USER IF NOT EXISTS api;\" -e \"GRANT admin TO api;\" -e \"CREATE ROLE IF NOT EXISTS db_admin;\" -e \"CREATE ROLE IF NOT EXISTS db_rw;\" -e \"CREATE ROLE IF NOT EXISTS db_read;\""
  },
  "config": {
    "port": "9090"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bathtaters/tournament-tracker.git"
  },
  "author": "Nick Chirumbolo",
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://github.com/bathtaters/tournament-tracker/issues"
  },
  "homepage": "https://github.com/bathtaters/tournament-tracker#readme",
  "dependencies": {
    "child-process": "^1.0.2",
    "connect-pg-simple": "^9.0.1",
    "crypto": "^1.0.1",
    "express": "^4.18.1",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pg-connection-string": "^2.6.4",
    "postgres-interval": "^4.0.0",
    "prompt": "^1.3.0",
    "uuid": "^10.0.0",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@types/connect-pg-simple": "^7.0.3",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.0.3",
    "jest": "^29.0.3"
  }
}
