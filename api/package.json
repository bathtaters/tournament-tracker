{
  "name": "tournament-tracker-api",
  "version": "0.0.1",
  "description": "Tournament Tracker backend",
  "main": "./src/server.js",
  "scripts": {
    "start": "node ./src/server.js",
    "test": "jest",
    "inspect": "node --inspect ./src/server.js",
    "bg-start": "[[ $(redis-cli ping) != \"PONG\" ]] && brew services start redis",
    "bg-stop": "[[ $(redis-cli ping) == \"PONG\" ]] && brew services stop redis"
  },
  "config": {
    "port": "9090"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bathtaters/tournament-tracker.git"
  },
  "author": "Nick Chirumbolo",
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://github.com/bathtaters/tournament-tracker/issues"
  },
  "homepage": "https://github.com/bathtaters/tournament-tracker#readme",
  "dependencies": {
    "child-process": "latest",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "express-redis-cache": "^1.1.3",
    "express-validator": "^6.12.1",
    "pg": "latest",
    "pg-connection-string": "latest",
    "postgres-interval": "^4.0.0",
    "prompt": "1.3.0",
    "uuid": "latest",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7"
  }
}
